{
    "name": "famdirksen/timezonelist",
    "description": "A small package use to create a timezone list box in Laravel",
    "keywords": ["timezone", "timezones", "timezonelist", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Dirksen",
            "email": "robin@famdirksen.nl"
        }, {
            "name": "Jackie Do",
            "email": "anhvudo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Famdirksen\\Timezonelist\\": "src"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Famdirksen\\Timezonelist\\TimezonelistServiceProvider"
            ],
            "aliases": {
                "Timezonelist": "Famdirksen\\Timezonelist\\Facades\\Timezonelist"
            }
        }
    }
}
